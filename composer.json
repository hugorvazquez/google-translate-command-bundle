{
  "name": "maximepinot/google-translate-command-bundle",
  "description": "Automatically translate your Symfony translation files with the Google Translate API.",
  "type": "symfony-bundle",
  "keywords": ["symfony", "google", "translate", "translations"],
  "homepage": "https://github.com/MaximePinot/google-translate-command-bundle",
  "readme": "https://github.com/MaximePinot/google-translate-command-bundle/blob/master/README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "Maxime Pinot",
      "email": "contact@maximepinot.com",
      "homepage": "https://www.maximepinot.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/MaximePinot/google-translate-command-bundle/issues",
    "source": "https://github.com/MaximePinot/google-translate-command-bundle",
    "docs": "https://github.com/MaximePinot/google-translate-command-bundle/tree/master/docs/index.md"
  },
  "require": {
    "php": "^8.0",
    "ext-intl": "*",
    "symfony/framework-bundle": "7.2.*",
    "symfony/console": "7.2.*",
    "symfony/translation": "7.2.*",
    "stichoza/google-translate-php": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "MaximePinot\\GoogleTranslateCommandBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MaximePinot\\GoogleTranslateCommandBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "7.2.*",
    "friendsofphp/php-cs-fixer": "^3.7",
    "symfony/yaml": "7.2.*"
  },
  "extra": {
    "thanks": {
      "name": "stichoza/google-translate-php",
      "url": "https://github.com/Stichoza/google-translate-php"
    }
  }
}
